-var titulo = "Titulo principal"
-var titulos = ["Bienvenido al mundo de los Crypto Juegos", "Subtitulo 1", "Subtitulo 2", "Subtitulo 3"]
-var enlaces = ["Acerca", "Noticias", "Contactos"]
mixin caja(imagen, titulo, contenido)
  .caja
    .caja__imagen: img(src="images/"+imagen)
    .caja__contenido
      h3=titulo
      p=contenido